{
  "expo": {
    "name": "Pomotato",
    "slug": "pomotato",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ipsally.pomotato"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.ipsally.pomotato"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Pomotato is a solution for ADHD go-getters to indulge in controlled distractions. By inspiring our users to pursue better break-time activities that help sustain their productivity throughout their day."
  }
}
